module SentDm
  module Resources
    class Users
      def retrieve: (
        String user_id,
        ?request_options: SentDm::request_opts
      ) -> SentDm::APIResponseOfUser

      def list: (
        ?request_options: SentDm::request_opts
      ) -> SentDm::Models::UserListResponse

      def invite: (
        ?email: String,
        ?name: String,
        ?role: String,
        ?test_mode: bool,
        ?idempotency_key: String,
        ?request_options: SentDm::request_opts
      ) -> SentDm::APIResponseOfUser

      def remove: (
        String path_user_id,
        ?test_mode: bool,
        ?body_user_id: String,
        ?request_options: SentDm::request_opts
      ) -> nil

      def update_role: (
        String path_user_id,
        ?role: String,
        ?test_mode: bool,
        ?body_user_id: String,
        ?idempotency_key: String,
        ?request_options: SentDm::request_opts
      ) -> SentDm::APIResponseOfUser

      def initialize: (client: SentDm::Client) -> void
    end
  end
end
